{
  "workbench.colorTheme": "Monokai",
  "workbench.colorCustomizations": {
    "titleBar.border": "#0e0f10",
    "list.activeSelectionBackground": "#0e0f10",
    "list.inactiveSelectionBackground": "#161a1d",
    "tab.inactiveBackground": "#111213",
    "editorGroupHeader.tabsBackground": "#0e0f10",
    "editorGroupHeader.noTabsBackground": "#0e0f10",
    "activityBar.background": "#0e0f10",
    "sideBar.background": "#0e0f10",
    "sideBarSectionHeader.background": "#0e0f10",
    "activityBar.border": "#0b1015",
    "tab.border": "#0b1015",
    "panel.border": "#0e0f10",
    "statusBar.border": "#0e0f10",
    "editor.background": "#1b1e23"
  },
  "prettier.arrowParens": "avoid",
  "prettier.bracketSpacing": true,
  "prettier.endOfLine": "auto",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.insertPragma": false,
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 80,
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "as-needed",
  "prettier.requirePragma": false,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "prettier.useTabs": false,
  "prettier.vueIndentScriptAndStyle": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    ".hg": true,
    "bower_compoments": true
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "html.format.enable": true,
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.triggerExpansionOnTab": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "editor.suggestSelection": "first",
  "files.exclude": {
    ".npm-offline-cache": true
  },
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "terminal.integrated.rendererType": "dom",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enableFiletypes": ["!java"],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "prettier.useEditorConfig": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": ["gapi", "resultvalues"],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "[less]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "editor.defaultFormatter": "michelemelluso.code-beautifier",
  "npm.keybindingsChangedWarningShown": true,
  "git.suggestSmartCommit": false,
  "git.autofetch": true,
  "eslint.format.enable": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "sort-imports.cache-package-json-config-checks": false,
  "sort-imports.on-save": false,
  "sort-imports.ignore-type-defs": false,
  "sort-imports.default-sort-style": "eslint",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "diffEditor.hideUnchangedRegions.enabled": true,
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "cmake.configureOnOpen": true,
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "window.commandCenter": false
}
